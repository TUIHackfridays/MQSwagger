{
  "name": "mqswagger",
  "private": true,
  "version": "0.0.1",
  "main": "app.js",
  "scripts": {
    "lint": "eslint -c .eslintrc ./",
    "start-prod": "node ./node_modules/better-npm-run start-prod",
    "start-dev": "node ./node_modules/better-npm-run start-dev",
    "test": "mocha ./test",
    "gen-doc": "rm -rf docs/visualtalk-api && jsdoc ./docs/README.md package.json -c conf.json"
  },
  "betterScripts": {
    "start-prod": {
      "command": "node ./app.js",
      "env": {
        "NODE_ENV": "production",
        "NODE_CONFIG_DIR": "./config"
      }
    },
    "start-dev": {
      "command": "node ./app.js",
      "env": {
        "NODE_ENV": "development",
        "NODE_CONFIG_DIR": "./config"
      }
    }
  },
  "author": "Edgar Ribeiro",
  "email": "edgar@cloudoki.com",
  "engines": {
    "node": ">=4.1 <5",
    "npm": ">=3.4"
  },
  "dependencies": {
    "better-npm-run": "0.0.4",
    "config": "^1.17.1",
    "express": "^4.13.3",
    "lodash": "^3.10.1",
    "morgan": "^1.6.1",
    "winston": "^2.1.1",
    "swagger-express-mw": "^0.1.0",
    "jsdoc": "^3.4.0"
  },
  "devDependencies": {
    "chai": "^3.4.1",
    "eslint": "^1.10.1",
    "eslint-config-airbnb": "^1.0.0",
    "mocha": "^2.3.4",
    "should": "^7.1.0",
    "supertest": "^1.0.0"
  }
}
